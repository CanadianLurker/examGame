<AnchorPane id="AnchorPane" onKeyPressed="#keyPressed" onKeyReleased="#keyReleased" prefHeight="550.0" prefWidth="800.0" style="-fx-background-color: a6a6a6;" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="humphriesmartinfice.examproject.FXMLStartController">
   <children>
      <ImageView fx:id="doorOpen" fitHeight="152.0" fitWidth="159.0" layoutX="324.0" layoutY="1.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../door_open.png" />
         </image>
      </ImageView>
      <ImageView fx:id="bed" fitHeight="88.0" fitWidth="163.0" layoutY="423.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../bed.png" />
