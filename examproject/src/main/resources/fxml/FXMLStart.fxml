      </ImageView>
      <ImageView fx:id="table" fitHeight="102.0" fitWidth="102.0" layoutX="660.0" layoutY="419.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../table.png" />
         </image>
      </ImageView>
      <ImageView fx:id="toilet" fitHeight="108.0" fitWidth="89.0" layoutY="221.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../toilet.png" />
         </image>
      </ImageView>
      <ImageView fx:id="lamp" fitHeight="107.0" fitWidth="62.0" layoutX="680.0" layoutY="384.0" pickOnBounds="true" preserveRatio="true">
