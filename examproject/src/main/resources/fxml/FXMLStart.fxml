         <children>
            <ImageView fx:id="player" fitHeight="136.0" fitWidth="135.0" layoutX="-7.0" layoutY="-3.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../prisoner.png" />
