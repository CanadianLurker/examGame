            <Image url="@../door%20closed.png" />
         </image>
      </ImageView>
      <ImageView fx:id="poster" fitHeight="70.0" fitWidth="110.0" layoutX="123.0" layoutY="41.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../poster.png" />
         </image>
      </ImageView>
      <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#8a8a8a" height="8.0" layoutY="145.0" stroke="#8a8a8a" strokeType="INSIDE" width="800.0" />
      <ImageView fx:id="bars" fitHeight="82.0" fitWidth="117.0" layoutX="577.0" layoutY="21.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../bars.png" />
