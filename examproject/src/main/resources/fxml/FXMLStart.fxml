      </ImageView>
      <ImageView fx:id="key" fitHeight="82.0" fitWidth="37.0" layoutX="693.0" layoutY="455.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../key.png" />
         </image>
      </ImageView>
      <Label fx:id="keyFound" layoutX="267.0" layoutY="460.0" text="You found a cell key!" visible="false">
         <font>
            <Font name="System Bold" size="27.0" />
         </font>
      </Label>
      <Button fx:id="btnLeave" layoutX="349.0" layoutY="312.0" mnemonicParsing="false" onAction="#btnLeaveY" text="Yes" visible="false" />
      <Button fx:id="btnStay" layoutX="400.0" layoutY="312.0" mnemonicParsing="false" onAction="#btnStayN" text="No" visible="false" />
      <Label fx:id="exitMessage" layoutX="267.0" layoutY="278.0" text="Would you like to exit this room?" visible="false">
         <font>
            <Font size="17.0" />
         </font>
      </Label>
